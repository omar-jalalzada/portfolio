.main_inner_container
  = partial "partials/main_header", :locals => { :main_title => "OMAR JALALZADA", :sub_title => "designer & front-end developer" }
  
  .main_nav
    .row.max_width_center
      .columns.small-6.large-3
        .custom_round_containers
          %a.work{:onclick=>"scroll_to_el('project_list')"}
            .bg_img
            .text Work
      .columns.small-6.large-3
        .custom_round_containers
          %a.about_me{:onclick=>"scroll_to_el('about_me')"}
            .bg_img
            .text About
      .columns.small-6.large-3
        .custom_round_containers
          %a.talk_to_me{:onclick=>"scroll_to_el('talk_to_me')"}
            .bg_img
            .text Contact
      .columns.small-6.large-3
        .custom_round_containers
          %a.blog{:href=>"http://blog.ojtwisted.com", :target=>"_blank"}
            .bg_img
            .text Blog

  = partial "partials/project_list"

