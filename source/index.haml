      .columns.small-4.large-4
        %a.call
          .bg_img
          .text 
            %span.has-tip.tip-top{:title=>"Copy to clipboard", " data-tooltip " => true} 
              (650)388-0106
        
      .columns.small-4.large-4
        %a.twitter{:href=>"https://twitter.com/ojtwisted", :target=> "_blank"}
          .bg_img
          .text @ojtwisted
        
    .row.max_width_center
      .columns.small-4.large-4
        %a{:href=>"http://www.linkedin.com/profile/view?id=71647679", :target=> "_blank"}
